{
    "builds": [
      {
        "src": "bpdata/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "bpdata/main.py"
      }
    ],
    
    "env": {
      "PYTHON_VERSION": "3.11",
      "PIP_OPTIONS": "--no-cache-dir"
    }
    
}