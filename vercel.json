{
  "builds": [
    {
      "src": "bpdata/main.py",
      "use": "@vercel/python",
      "config": {
              "runtime": "python3.11",
              "buildCommand": "python3.11 -m pip install -r requirements.txt"
          }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "bpdata/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11" 
  }
    
}